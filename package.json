{
  "name": "cdkit.ui.automation",
  "version": "0.8.0",
  "description": "Appium infrastructure to run Appium tests on iOS simulators and Genymotion emulators.",
  "main": "cli.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test:coverage": "./node_modules/.bin/nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:timoa/cdkit.ui.automation.git"
  },
  "keywords": [
    "appium",
    "titanium",
    "mobile"
  ],
  "author": "Wilson Luu",
  "contributors": [{
    "name": "Damien Laureaux",
    "url": "https://timoa.com"
  }],
  "license": "ISC",
  "homepage": "https://github.com/timoa/cdkit.ui.automation#readme",
  "dependencies": {
    "appium": "^1.15.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "commander": "^3.0.2",
    "mocha": "^6.2.1",
    "q": "^2.0.2",
    "underscore": "^1.9.1",
    "wd": "^1.11.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "acorn": "^6.0.4",
    "acorn-jsx": "^5.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "espree": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "config/*.js",
      "helpers/*.js",
      "lib/*.js",
      "cli.js"
    ],
    "exclude": [
      "test/*.spec.js",
      "coverage/**",
      "ui-tests/**/*.js"
    ],
    "all": true
  }
}
